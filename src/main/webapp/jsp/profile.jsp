<table width="100%" class="table-profile">
	<tr>
		<td align="left" width="165px" rowspan="5">
			<img style="-webkit-user-select: none" src="${user.profilePicture}">
		</td>
		<td>
			<font face="verdana">id:${user.id}</font>
		</td>
	</tr>
	<tr>
		<td>
			<font face="verdana">name:${user.username}</font>
		</td>
	</tr>
	<tr>
		<td>
			<font face="verdana">posts:</font>
		</td>
	</tr>
	<tr>
		<td>
			<font face="verdana">followers:</font>
		</td colspan="3">
	</tr>
	<tr>
		<td>
			<font face="verdana">followed by:</font>
		</td>
	</tr>	
</table>
<table align="left" class="table-profile">
	<tr>
		<td>
			<button class="btn-menu-lightsup" style="width:150px">
				Pick Statistic
			</button>
		</td>
		<td>
			<button class="btn-menu-lightsup" style="width:150px">
				Followers
			</button>
		</td>
		<td>
			<button class="btn-menu-lightsup" style="width:150px">
				Followed by
			</button>
		</td>
	</tr>
</table>
